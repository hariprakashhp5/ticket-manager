
<%= form_for(@tracker) do |f| %>
  

<div class="form-inline">
  <div class="form-group">
    <%= f.number_field :ticket_id, :placeholder=>"Ticket ID" ,class: "form-control" %>
  </div>
  
    
    <%= f.hidden_field :created, :value =>"", class: "form-control" %>
  
  <div class="form-group">
    <%= f.text_field :eta, :placeholder=>"ETA", class: "form-control" %>
  </div>
 
    <%= f.hidden_field :finished, :value =>"", class: "form-control" %>
    <%= f.hidden_field :owner, :value =>"",class: "form-control"%>

  <div class="form-group">
    <%=f.select :tow, options_for_work,{}, class: "form-control"%>
  </div>
  <div class="form-group">
    <%= f.number_field :noc,:placeholder=>"Commits", class: "form-control"%>
  </div>
  <div class="form-group">
    <%= f.text_field :staging,:placeholder=>"Staging", class: "form-control"%>
  </div>
  
  
    <%= f.hidden_field :disc,:value =>"", class: "form-control"%>  
    <%= f.hidden_field :uid, :value => current_user.id, class: "form-control"%>


  
  
    <%= f.submit "Save", class: "btn btn-primary"%>
 </div>

<% end %>
