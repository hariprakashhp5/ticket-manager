
<h1 style="text-align:center;">QC Tickets</h1>

<!-- <div class="pull-left"><span>Pending:</span> <a href="/pending"><%=pending%></a></div>

<div class="status"><span>Outbound:</span> <%=@delay%></div>

<div class="status"><span>Neutral:</span> <%=@ontime%></div>

<div class="status"><span>Inbound:</span> <%=@early%></div>
 -->

<div style="clear:both"></div>
<table>
  <thead>
    <tr>
      <th>Date</th>
      <th colspan="2">Ticket ID</th>
      <th>Description</th>
      <th>Created By</th>
      
      <th>ETA</th>
     
      <th>Type of Work</th>
      <th>Commits</th>
      <th>Bugs</th>
      <th></th>
      <th colspan="2">Status</th>
    
     <!--  <th colspan="3"></th> -->
    </tr>
  </thead>

  <tbody>
    <% @trackers.each do |tracker| %>
      <tr>
        <td><%=tracker.created_at.strftime("%d-%m-%y")%></td>
        <td><a href="/trackers/<%=tracker.id%>"><%=tracker.ticket_id%></a>
        <td width="20px;">   <a target="_blank" href="https://bankbazaarmail.zendesk.com/agent/tickets/<%= tracker.ticket_id%>">Open</a></td></td>
        <td ><%= best_in_place tracker, :disc, :as => :textarea%></td>
        <td ><%= best_in_place tracker, :owner, :inner_class => "tdfield" %></td>
                  
        <td ><%= best_in_place tracker, :eta, :inner_class => "tdfield"%></td>
        
        <td ><%=  best_in_place tracker, :tow, :inner_class => "tdfield"%></td>
        <td ><%= best_in_place tracker, :noc, :inner_class => "tdfield"%></td>        
        <!-- <td><%#= link_to 'Show', tracker %></td> -->
        <td ><%= best_in_place tracker, :bugs, :inner_class => "tdfield" %></td>
       <td><%= link_to 'Qc Remarks', edit_tracker_path(tracker)%></td>
        <td >
           <%if  tracker.status=="1" %>
           <%= button_to 'Send Push', push_path(:id=>tracker.id), :method => :post, :remote=>true %>
           <%elsif tracker.status == "1"%>
           In QC
           <%elsif tracker.status == "2"%>
           Error 
           <%elsif tracker.status == "3"%>
           Yet to Push
           <%elsif tracker.status == "4"%>
           Live
           <%end%>
           <td><%= button_to 'Error', error_path(:id=>tracker.id), :method => :post, :remote=>true %></td>
         </td>
             
      </tr>
    <% end %>
  </tbody>
</table>

<br>


