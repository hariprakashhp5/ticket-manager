<h1 style="text-align:center;">Inbound Tickets</h1>
<table class="table table-bordered">
  <thead>
    <tr>
      <th>Date</th>
      <th colspan="2">Ticket ID</th>
      <th>Ticket Owner</th>
      <th>Created</th>
      <th>ETA</th>
      <th>Finished</th>
      <th>Type of Work</th>
      <th>No. of Commits</th>
      <th>Staging</th>
    <th>Completed</th>
      <th>Description</th>
      <th>Config Owner</th>
    </tr>
  </thead>

  <tbody>
    <% @trackers.each do |tracker| %>
      <tr>
        <td><%= tracker.created_at%></td>
        <td ><%= tracker.ticket_id %><td width="20px;"><a target="_blank" href="https://bankbazaarmail.zendesk.com/agent/tickets/<%= tracker.ticket_id%>">Open</a></td></td>
        <td ><%= tracker.owner %></td>
                  <td ><%= tracker.created %></td>
        <td ><%= tracker.eta%></td>
        <td ><%= tracker.finished%></td>
        <td ><%= tracker.tow%></td>
        <td ><%= tracker.noc%></td>
        <td ><%= tracker.staging%></td>
        <td ><%= tracker.comp %></td>
        <td ><%= tracker.disc%></td>
        <td><%= User.where("id=?", tracker.uid).pluck("name").first.capitalize%></td>
      </tr>
    <% end %>
  </tbody>
</table>