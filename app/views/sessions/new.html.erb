
  <div class="form">
  <h3>LOG IN</h3>
<%= form_for(:session, url: login_path) do |f| %> 
  <%= f.email_field :email, :placeholder => "Email", class: "tt" %> 
  <%= f.password_field :password, :placeholder => "Password", class: "tt" %> <br>
  <%= f.submit "Log in", class: "btn-submit-one" %>
<% end %>
<p>New?-<%= link_to "SignUp",'signup'%></p>
<!-- <p><%#= link_to "Forgot password?", new_password_reset_path %></p> -->
</div>
